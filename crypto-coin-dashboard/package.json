{
  "name": "crypto-coin-dashboard",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "clean": "rimraf dist",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "start:mac": "export PORT=8082 && react-scripts start",
    "start:win": "set PORT=8082 && react-scripts start",
    "test": "react-scripts --expose-gc test --logHeapUsage --max-old-space-size=8192",
    "test:unit": "react-scripts --expose-gc test --logHeapUsage --runInBand --max-old-space-size=8192 --coverage .",
    "test:memory": "react-scripts --expose-gc test --logHeapUsage --max-old-space-size=8192",
    "test:coverage": "react-scripts --expose-gc test --max-old-space-size=8192 --coverage .",
    "cypress:open": "cypress open",
    "test:e2e-ci": "cypress run",
    "lint": "eslint --ext .js src",
    "lint-fix": "eslint --fix --ext .js ."
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^6.3.0",
    "@mui/x-data-grid": "^7.23.5",
    "@reduxjs/toolkit": "^2.5.0",
    "crypto-coin-dashboard": "file:",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "react-toastify": "^11.0.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@eslint/js": "^9.17.0",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "babel-jest": "^29.6.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "cypress": "^12.17.1",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^8.44.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "jest": "^29.6.1",
    "msw": "^1.2.2",
    "npm-check-updates": "^16.14.20",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^18.3.1",
    "redux-mock-store": "^1.5.5",
    "vite": "^6.0.5"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!leaflet|)"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    },
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/tests/**",
      "!src/index.js",
      "!src/reportWebVitals.js"
    ],
    "coveragePathIgnorePatterns": [
      "src/static",
      "src/components/apps/shiv/styles",
      "node_modules/"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
